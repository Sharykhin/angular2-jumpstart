    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^2.8.0",
    "less-plugin-clean-css": "^1.5.1",
    "lite-server": "^2.1.0",
    "tsconfig-glob": "^0.4.1",
    "tslint": "^3.6.0",
