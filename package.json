{
  "name": "angular2-jumpstart",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npm run typings install",
    "tslint": "tslint -c tslint.json src/app/**/*.ts",
    "serve": "lite-server",
    "start": "concurrent \"rm -rf ./css\"  \"rm -rf ./app \" \"gulp build\" \"gulp watch\" ",
