{
  "name": "angular2-jumpstart",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "npm run typings install",
    "tslint": "tslint -c tslint.json src/app/**/*.ts",
    "serve": "cd public && lite-server",
    "start": "concurrent \"rm -rf public/*\" \"gulp build\" \"gulp watch\" ",
    "typings": "typings",
    "build": "concurrent \"rm -rf public/*\" \"gulp build\" "
  },
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "systemjs": "0.19.22",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "del": "^2.1.0",
    "es6-promise": "^3.1.2",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^4.3.3",
    "gulp-typescript": "^2.8.0",
    "lite-server": "^2.1.0",
    "tsconfig-glob": "^0.4.1",
    "tslint": "^3.6.0",
    "typescript": "^1.8.2",
    "typings": "^0.6.8"
  }
}
